{
  "name": "ember-leaflet",
  "version": "5.1.0",
  "description": "Easy and declarative mapping for ember.",
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:deps": "ember dependency-lint",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests; run tests in test-app'",
    "prepare": "yarn build"
  },
  "homepage": "https://miguelcobain.github.io/ember-leaflet/",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "ember-leaflet-marker-cluster": "^1.0.1",
    "ember-template-lint": "^4.0.0",
    "ember-template-lint-plugin-prettier": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "leaflet": "^1.7.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@embroider/addon-shim": "1.6.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "ember-composability-tools": "^1.1.0",
    "ember-in-element-polyfill": "^1.0.0",
    "ember-render-helpers": "^0.2.0",
    "fastboot-transform": "^0.1.3",
    "resolve": "^1.21.0"
  },
  "peerDependencies": {
    "leaflet": ">=0.7"
  },
  "keywords": [
    "ember-addon",
    "leaflet",
    "ember",
    "ember-leaflet",
    "mapping",
    "maps"
  ],
  "repository": "https://github.com/miguelcobain/ember-leaflet",
  "license": "MIT",
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "exports": {
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,json,yaml,yml,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
