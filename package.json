{
  "name": "ember-leaflet",
  "version": "4.1.1",
  "description": "Easy and declarative mapping for ember.",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "eslint-config-check": "eslint --print-config . | eslint-config-prettier-check",
    "prettier-all": "prettier --write \"**/*.{js,scss,json,yaml,yml}\"",
    "eslint-all": "eslint --fix \"**/*.js\"",
    "format-all": "npm run prettier-all && npm run eslint-all"
  },
  "homepage": "https://miguelcobain.github.io/ember-leaflet/",
  "engines": {
    "node": "10.* || >= 12"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-angle-bracket-invocation-polyfill": "^2.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-addon-docs": "^0.10.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-custom-assertions": "^0.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-favicon": "^2.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-is-component": "^0.5.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-data": "^3.21.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.13.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-ember": "^9.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "leaflet": "^1.7.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "qunit-dom": "^1.5.0",
    "sass": "^1.26.11"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-composability-tools": "^1.0.0",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-fn-helper-polyfill": "^1.0.2",
    "ember-get-config": "^0.2.4",
    "ember-in-element-polyfill": "^1.0.0",
    "ember-invoke-action": "^1.5.1",
    "ember-let-polyfill": "^0.1.0",
    "ember-render-helpers": "^0.1.1",
    "fastboot-transform": "^0.1.3",
    "resolve": "^1.17.0"
  },
  "peerDependencies": {
    "leaflet": ">=0.7"
  },
  "keywords": [
    "ember-addon",
    "leaflet",
    "ember",
    "ember-leaflet",
    "mapping",
    "maps"
  ],
  "repository": "https://github.com/miguelcobain/ember-leaflet",
  "license": "MIT",
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://miguelcobain.github.io/ember-leaflet/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,json,yaml,yml,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
